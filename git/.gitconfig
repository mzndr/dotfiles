[init]
  defaultBranch = main
[mergetool "nvim"]
  cmd = nvim -f -c \"Gdiffsplit!\" \"$MERGED\"
[mergetool]
  prompt = false
[merge]
  tool = nvim
[user]
	email = hello@mzndr.me
	name = Marius Zander
  useConfigOnly = true
[push]
  autoSetupRemote = true
[pull]
  rebase = true
[fetch]
  prune = true
[add]
  patch = true
[commit]
  verbose = true
[remote "origin"]
  prune = true
